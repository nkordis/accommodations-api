{
  "ConnectionStrings": {
    "AccommodationsDb": "Server=(localdb)\\mssqllocaldb;Database=AccommodationsDb;Trusted_Connection=True;"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd-MM HH:mm:ss} {Level:u3}] |{SourceContext}| {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Accommodations-API-.log",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "ApplicationInsights",
        "Args": {
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ]
  },
  "Swagger": {
    "Version": "v0.1.0",
    "Title": "API",
    "Description": "API Documentation",
    "Endpoint": "/swagger/v0.1.0/swagger.json",
    "RoutePrefix": "api/swagger"
  },
  "BlobStorage": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=accommodationsapisa;AccountKey=VovaFfU5pY4h26prbI2zqckMwEwbhiJKyli82ENXlq31Uy9UNZ9HxE7L+a3XcQbyfEkaanh/9TR++ASt4yaSzA==;EndpointSuffix=core.windows.net",
    "AccommodationsContainerName": "accommodations",
    "AccountKey": "VovaFfU5pY4h26prbI2zqckMwEwbhiJKyli82ENXlq31Uy9UNZ9HxE7L+a3XcQbyfEkaanh/9TR++ASt4yaSzA=="
  }
}
